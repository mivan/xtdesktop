SELECT usr_id, usr_username
FROM usr 
WHERE (usr_username=current_user)
UNION
SELECT usr_id, usr_username AS usr_username
FROM usr
WHERE ((usr_active = true)
    AND(usr_username <> current_user))
ORDER BY usr_username; 



